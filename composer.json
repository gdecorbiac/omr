{
  "name": "gdecorbiac/omr",
  "description": "Optical Mark Recognition from PHP",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Jansen Felipe",
      "email": "jansen.felipe@gmail.com"
    },
    {
        "name": "Geoffroy de Corbiac",
        "email": "geoffroydecorbiac@gmail.com"
    }
  ],
  "keywords": [
    "omr",
    "optical mark recognition",
    "survey",
    "barcode",
    "zbar",
    "qrcode"
  ],
  "require": {
    "php": "7.*",
    "ext-imagick": "*",
    "symfony/console": "2.7.*",
    "robbiep/zbar-qrdecoder": "dev-master"
  },
  "bin": ["bin/omr"],
  "require-dev": {
    "phpunit/phpunit": "8.*"
  },
  "autoload": {
    "psr-4": {
      "JansenFelipe\\OMR\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JansenFelipe\\OMR\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  },
  "minimum-stability": "dev"
}
